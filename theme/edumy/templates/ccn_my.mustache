{{> theme_edumy/head_dashboard }}
<body {{{ bodyattributes }}}>
  <div class="wrapper">
    {{> theme_edumy/ccn_dashboard_navbar }}
    <!--
    <section class="dashboard_sidebar dn-1199">
      <div class="dashboard_sidebars">
        <div class="user_board">
